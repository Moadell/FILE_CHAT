{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["src"]
}
